<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Title>GovInfoFetch</Title>
		<Authors>Louis S. Berman</Authors>
		<Description>Fetches info on US politicians</Description>
		<Copyright>Copyright 2024 by Louis S. Berman</Copyright>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/squideyes/GovInfoFetch</RepositoryUrl>
		<PackageTags>government,politicians</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
